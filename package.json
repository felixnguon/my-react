{
  "name": "myreact",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run postcss:watch & npm run parcel:serve",
    "parcel:serve": "parcel src/index.html",
    "parcel:watch": "parcel watch src/index.html",
    "parcel:build": "parcel build src/index.html",
    "postcss:watch": "postcss src/assets/css/tailwindcss.src.css -o src/assets/css/index.css --env development -w",
    "postcss:build": "postcss src/assets/css/tailwindcss.src.css -o src/assets/css/index.css --env production",
    "format": "prettier \"src/**/*.{js,html}\" --write",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "clean": "rm -rf dist .cache",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "parcel-bundler": "^1.12.4",
    "postcss-cli": "^7.1.0",
    "postcss-fontpath": "^1.0.0",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tailwindcss": "^1.2.0"
  }
}
